{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:theme-typings": "chakra-cli tokens ./src/styles/theme.ts",
    "generate": "graphql-codegen",
    "postinstall": "npm run gen:theme-typings",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx.--fix",
    "format:prettier": "prettier --write \"**/*.{ts,js,json}\"",
    "lint:format": "npm run lint:eslint && npm run format:prettier",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css,less,scss,html,json,jsx,tsx,js,ts}": [
      "npm run lint:format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "2.4.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@faststore/api": "^1.12.38",
    "@faststore/graphql-utils": "^1.12.37",
    "@faststore/sdk": "^1.12.37",
    "@fontsource/inter": "^4.5.15",
    "@graphql-yoga/plugin-disable-introspection": "^0.7.0",
    "@hookform/resolvers": "^3.1.0",
    "@next/font": "13.1.1",
    "@splidejs/react-splide": "^0.7.12",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-input-mask": "^3.0.2",
    "eslint-config-next": "13.1.1",
    "framer-motion": "^6.5.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "graphql-yoga": "^3.8.0",
    "jotai": "^2.0.4",
    "localforage": "^1.10.0",
    "localforage-driver-memory": "^1.0.5",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-input-mask": "^2.0.4",
    "react-query": "^3.39.3",
    "react-use": "^17.4.0",
    "storybook-addon-next-router": "^4.0.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "4.9.4",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@chakra-ui/cli": "^2.3.0",
    "@chakra-ui/storybook-addon": "^4.0.16",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^8.3.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.1"
  }
}
